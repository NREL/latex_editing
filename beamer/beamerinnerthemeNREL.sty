\newcommand{\btVFill}{\vskip0pt plus 1filll}

% Title page
\defbeamerRAAW*{title page}{NREL}[1][]
{ 
	\vskip1.7cm%
    	\begin{beamercolorbox}[wd=12cm,leftskip=3cm,sep=8pt,#1]{title page header}
      		\Large{\textbf{\inserttitle}}\par%
    	\end{beamercolorbox}%
    \vskip3.0cm%
	    \begin{beamercolorbox}[wd=12cm,leftskip=4cm,#1]{author}
	    	\large{\textbf{\insertauthor}}%
	    \end{beamercolorbox}
    \vskip0.2cm%
	    \begin{beamercolorbox}[wd=12cm,leftskip=4cm,#1]{date}
	    	\large{\textbf{\insertdate}}%
	    \end{beamercolorbox}
	\btVFill
	    \begin{beamercolorbox}[wd=12cm,leftskip=0.5cm,#1]{title page info}
	    	\scalebox{.43}{NREL is a national laboratory of the U.S. Department of Energy, Office of Energy Efficiency and Renewable Energy, operated by the Alliance for Sustainable Energy, LLC.}
	    \end{beamercolorbox}
	\vskip0.2cm%
}

